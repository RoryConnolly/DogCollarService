  "/pushCollarData": {
    "post": {
        "tags": [
            "Dog Collar Requests"
        ],
        "summary": "Gets a specific Dog Collar Responses from the DB",
        "requestBody": {
                "required": true,
                "description": "Cat with id",
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "collarId": {
                          "type": "string"
                        },
                        "collarResp": {
                          "type": "string"
                        }
                      },
                      "example": {
                        "collarId": "abc4",
                        "collarResp": "1",
                        "dogName": "Tyson",
                        "barking": "medium",
                        "activity": "low",
                        "location": "37901"
                      }
                    }
                  }
              },
                "style": "form",
                "explode": "true"
          },
        "responses": {
            "200": {
                "description": "OK"
            },
            "404": {
                "description": "Failed. Cat not found."
            }
        }
    }
  },
