{
    "openapi": "3.0.0",
    "info": {
        "version": "1.0.0",
        "title": "Dog Collar Api",
        "description": "A simple API to receive JSON Payloads from Dog Collars and save and query them to a DynamoDB",
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        }
    },
    "servers": [
        {
            "url": "/api/",
            "description": "Local Dev"
        },
        {
            "url": "/api/",
            "description": "With AWS"
        }
    ],
    "tags": [
        {
            "name": "Dog Collar Requests",
            "description": "API for Dog Collar Responses"
        }
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/fetch/all": {
            "get": {
                "tags": [
                    "Dog Collar Requests"
                ],
                "summary": "Gets all Dog Collar Responses from the DB",
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "400":{
                      "description": "Bad Request"
                    }
                }
            }
          },
        "/fetch/SpecificCollarRespByID": {
            "get": {
                "tags": [
                    "Dog Collar Requests"
                ],
                "summary": "Gets a specific Dog Collar Responses from the DB",
                "parameters": [
                    {
                        "in": "query",
                        "name": "Values",
                        "required": true,
                        "description": "Cat with id",
                        "schema": {
                            "type": "object",
                            "properties": {
                              "collarId": {
                                "type": "string"
                              },
                              "collarResp": {
                                "type": "string"
                              }
                            },
                            "example": {
                              "collarId": "abc1",
                              "collarResp": "2"
                            }
                        },
                        "style": "form",
                        "explode": "true"
                      }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "404": {
                        "description": "Failed. Cat not found."
                    }
                }
            }
    },
    "/fetch/allByCollarID": {
      "get": {
          "tags": [
              "Dog Collar Requests"
