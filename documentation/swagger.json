    "/fetch/allByBarking": {
      "get": {
          "tags": [
              "Dog Collar Requests"
          ],
          "summary": "Gets a specific Dog Collar Responses from the DB",
          "parameters": [
              {
                  "in": "query",
                  "name": "Values",
